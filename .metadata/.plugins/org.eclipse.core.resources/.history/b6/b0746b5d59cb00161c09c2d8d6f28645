import java.sql.SQLException;

public class Test {
	public static void main(String args[]) {
		try {
			database.MySqlQuery msq = new database.MySqlQuery();
			System.out.println("ここまで成功");
			msq.mySqlTest();
			System.out.println("接続に成功しました");
		} catch (SQLException e) {
			e.printStackTrace();
		}
	}

}
